{
  // Use IntelliSense to learn about possible attributes.
  // Hover to view descriptions of existing attributes.
  // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
  "version": "0.2.0",
  "configurations": [
    {
      "name": "g++ - Build and debug active file",
      "type": "cppdbg",
      "request": "launch",
      "program": "${fileDirname}/${fileBasenameNoExtension}",
      "args": [],
      "stopAtEntry": false,
      "cwd": "${fileDirname}",
      "environment": [],
      "externalConsole": false,
      "MIMode": "gdb",
      "setupCommands": [
        {
          "description": "Enable pretty-printing for gdb",
          "text": "-enable-pretty-printing",
          "ignoreFailures": true
        }
      ],
      "preLaunchTask": "C/C++: g++ build active file",
      "miDebuggerPath": "/usr/bin/gdb"
    },
    {
      "name": "Segeom::main",
      "preLaunchTask": "Segeom::main Build Debug",
      "type": "cppvsdbg",
      "program": "${workspaceFolder}/bazel-bin/main/${fileBasenameNoExtension}.exe",
      "cwd": "${workspaceRoot}/bazel-out/x64_windows-dbg/bin/main/${fileBasenameNoExtension}.exe.runfiles/",
      "request": "launch"
    },
    {
      "name": "Debug test",
      "type": "cppvsdbg",
      "request": "launch",
      "program": "${workspaceRoot}/bazel-bin/test/PointNDTest",
      "cwd": "${workspaceRoot}"
    }
  ]
}